{
    "name": "joesu/laravel-scaffold",
    "description": "A powerful Laravel package that provides a complete implementation of Repository and Service patterns for rapid API development.",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/joesu/laravel-scaffold",
    "keywords": [
        "laravel",
        "api",
        "repository",
        "service",
        "pattern",
        "scaffold",
        "generator",
        "boilerplate",
        "architecture"
    ],
    "authors": [
        {
            "name": "JoeSu"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "JoeSu\\LaravelScaffold\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JoeSu\\LaravelScaffold\\Providers\\LaravelScaffoldServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "support": {
        "issues": "https://github.com/joesu/laravel-scaffold/issues",
        "source": "https://github.com/joesu/laravel-scaffold"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}