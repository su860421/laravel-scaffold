{
    "name": "joesu/laravel-scaffold",
    "description": "A powerful Laravel package that provides a complete implementation of Repository and Service patterns for rapid API development.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "api",
        "repository",
        "service",
        "pattern"
    ],
    "authors": [
        {
            "name": "JoeSu",
            "email": "info@joesu.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "JoeSu\\LaravelScaffold\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JoeSu\\LaravelScaffold\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JoeSu\\LaravelScaffold\\Providers\\LaravelScaffoldServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}